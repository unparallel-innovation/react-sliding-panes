{
  "name": "@unparallel/react-sliding-panes",
  "version": "1.1.0",
  "description": "Sliding pane component used for react, providing the following features: stack panes in top of each other, open a pane side by side and open a pane in fullscreen mode",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "css": "dist/main.css",
  "targets": {
    "main": {
      "source": "src/index.ts"
    },
    "module": {
      "source": "src/index.ts"
    },
    "types": {
      "source": "src/index.ts"
    },
    "css": {
      "source": "src/main.scss"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "rm -rf dist && npm run build",
    "watch": "parcel watch",
    "build": "parcel build",
    "start": "parcel dev/index.html",
    "clean": "rm -rf dist node_modules .parcel-cache"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@parcel/packager-ts": "^2.7.0",
    "@parcel/transformer-sass": "^2.7.0",
    "@parcel/transformer-typescript-types": "^2.7.0",
    "@release-it/keep-a-changelog": "^3.1.0",
    "@types/lodash": "^4.14.186",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "parcel": "^2.7.0",
    "process": "^0.11.10",
    "release-it": "^15.4.3",
    "typescript": "~4.7.4"
  },
  "peerDependencies": {
    "classnames": "^2",
    "lodash": "^4.17.21",
    "react": " ^16.8 || ^17 || ^18",
    "react-dom": "^16.8 || ^17 || ^18"
  }
}
